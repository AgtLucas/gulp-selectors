{
  "name": "gulp-mini-selectors",
  "description": "Minify CSS selectors.",
  "version": "0.0.1",
  "author": {
    "name": "Caleb Brewer",
    "email": "calebthebrewer@gmail.com"
  },
  "dependencies": {
    "gulp-util": "~3.0.1",
    "through2": ">=0.6.1 <1.0.0-0",
    "lodash": "^2.4.1"
  },
  "homepage": "https://github.com/calebthebrewer/gulp-mini-selectors",
  "bugs": "https://github.com/calebthebrewer/gulp-mini-selectors/issues",
  "keywords": [
    "gulp",
    "gulpplugin",
    "much",
    "minify",
    "css",
    "selectors",
    "styles"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/calebthebrewer/gulp-mini-selectors.git"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "vows": "~0.7.0",
    "jscoverage": "~0.5.6",
    "istanbul": "^0.3.2",
    "coveralls": "^2.11.2",
    "codeclimate-test-reporter": "0.0.4"
  },
  "scripts": {
    "test": "vows --spec",
    "coverage": "istanbul cover ./node_modules/vows/bin/vows --spec",
    "text-cov": "istanbul cover ./node_modules/vows/bin/vows --spec",
    "test-coveralls": "istanbul cover ./node_modules/vows/bin/vows --spec -- -R spec && cat ./coverage/lcov.info | ./bin/coveralls.js --verbose",
    "codeclimate": "cat ./coverage/lcov.info|codeclimate"
  }
}
